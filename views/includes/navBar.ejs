                    <a class="<%= path === '/myart' ? 'active' : '' %>" href="/myart">My Art
                    </a>
                </li>
            <!---<% } %>--->
        </ul>
        <ul class="">
            <!---<% if (!isAuthenticated) { %>--->
                <li class="">
                    <a class="<%= path === '/login' ? 'active' : '' %>" href="/login">Login</a>
                </li>
                <li class="">
                    <a class="<%= path === '/signup' ? 'active' : '' %>" href="/signup">Signup</a>
                </li>
